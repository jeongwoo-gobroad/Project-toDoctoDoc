<div class="container">
    <div class="content">
        <% doctorInfo.forEach((doctor) => { %>
            <form action="/admin/doctorVerification?_method=PATCH" method="post">
                <div class="form-group">
                    <%= doctor.id %>
                </div>
                <div class="form-group">
                    <%= doctor.name %>
                </div>
                <div class="form-group">
                    <%= doctor.personalID %>
                </div>
                <div class="form-group">
                    <%= doctor.doctorID %>
                </div>
                <div class="form-group">
                    <%= doctor.email %>
                </div>
                <div class="form-group">
                    <%= doctor.address %>
                </div>
                <div class="form-group">
                    <%= doctor.isVerified %>
                </div>
                <input type="hidden" id="doctorID" name="doctorID" value="<%= doctor._id %>"></hidden>
                <button type="submit" class="green-button">승인</button>
            </form>
            <hr>
        <% }) %>
    </div>
</div>