<div class="container">
    <div class="content">
        <% psys.forEach((psy) => { %>
            <form action="/admin/premiumifyPsy?_method=PATCH" method="post">
                <div class="form-group">
                    <%= psy.name %>
                </div>
                <div class="form-group">
                    <%= psy.place_id %>
                </div>
                <div class="form-group">
                    <%= psy.isPremiumPsy %>
                </div>
                <div class="form-group">
                    <%= psy.address %>
                </div>
                <div class="form-group">
                    <%= psy.phone %>
                </div>
                <div class="form-group">
                    <%= psy.doctors %>
                </div>
                <div class="form-group">
                    <%= psy.createdAt %>
                </div>
                <div class="form-group">
                    <%= psy.updatedAt %>
                </div>
                <input type="hidden" id="psyId" name="psyId" value="<%= psy._id %>"></hidden>
                <button type="submit" class="green-button">승인</button>
            </form>
            <hr>
            <form action="/admin/premiumifyPsy?_method=DELETE" method="post">
                <input type="hidden" id="psyId" name="psyId" value="<%= psy._id %>"></hidden>
                <button type="submit" class="green-button">승인취소</button>
            </form>
            <hr>
        <% }) %>
    </div>
</div>